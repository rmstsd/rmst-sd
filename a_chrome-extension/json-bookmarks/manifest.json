{
  "name": "json-bookmarks",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "image/cute.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "image/logo.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["/js/send-method.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["tabs", "bookmarks", "contextMenus"],
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://192.168.0.125/*", "http://192.168.254.102/*"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/json.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["./js/inject.js"]
    }
  ]
}

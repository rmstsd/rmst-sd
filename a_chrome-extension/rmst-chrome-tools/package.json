{
  "name": "rmst-chrome-tools",
  "displayName": "Rmst chrome tools",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "debugger",
      "tabs",
      "bookmarks",
      "contextMenus",
      "activeTab",
      "cookies",
      "storage",
      "webRequest",
      "favicon",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "_favicon/*"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "*"
        ]
      }
    ]
  }
}

{
  "name": "rmst-vscode-tools",
  "displayName": "rmst-vscode-tools",
  "description": "对单词重命名",
  "repository": {},
  "version": "0.2.1",
  "icon": "icon.png",
  "publisher": "rmst",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "vsce-build-vsix": "vsce package",
    "test": "vitest --run",
    "publish": "vsce publish patch"
  },
  "dependencies": {
    "@babel/parser": "^7.20.0",
    "@babel/traverse": "^7.20.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.18.2",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "delete-func",
        "title": "rmst: 删除光标所在的函数"
      },
      {
        "command": "convert-word",
        "title": "rmst: 转成其他形式"
      }
    ]
  }
}

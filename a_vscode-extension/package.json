{
  "name": "rmst-vscode-tools",
  "displayName": "rmst-vscode-tools",
  "description": "rmst-vscode-tools",
  "repository": {},
  "version": "0.2.6",
  "icon": "icon.png",
  "publisher": "rmst",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "vsce-build-vsix": "vsce package",
    "test": "vitest --run",
    "publish": "vsce publish patch"
  },
  "dependencies": {
    "@babel/parser": "^7.20.0",
    "@babel/traverse": "^7.20.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.18.2",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/vsce": "^3.3.2",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "delete-func",
        "title": "rmst: 删除光标所在的函数"
      },
      {
        "command": "convert-word",
        "title": "rmst: 转成其他形式"
      },
      {
        "command": "openFolderInNewWindow",
        "title": "rmst: 在新窗口打开"
      },
      {
        "command": "openInWindowTerminal",
        "title": "rmst: 在终端打开"
      },
      {
        "command": "i18n",
        "title": "rmst: 国际化"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "openFolderInNewWindow",
          "when": "explorerResourceIsFolder",
          "group": "navigation@999"
        }
      ]
    },
    "configuration": {
      "title": "rmst-vscode-tools",
      "properties": {
        "i18nFilePath": {
          "type": "string",
          "default": "",
          "description": "i18n文件路径"
        }
      }
    },
    "languages": [
      {
        "id": "customIni",
        "aliases": [
          "txt"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  }
}
